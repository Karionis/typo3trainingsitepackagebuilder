{
    "name": "niklaskanthak/typo3trainingsitepackagebuilder",
    "type": "typo3-cms-extension",
    "description": "Exercise for creating a site package extension based on sitepackagebuilder.com",
    "license": ["GPL-2.0-or-later"],
    "keywords": ["TYPO3 CMS"],
    "require": {
        "typo3/cms-core": "^12.4",
        "typo3/cms-rte-ckeditor": "^12.4",
        "typo3/cms-fluid-styled-content": "^12.4"
    },
    "autoload": {
        "psr-4": {
            "NiklasKanthak\\Typo3trainingsitepackagebuilder\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "typo3trainingsitepackagebuilder"
        }
    }
}
